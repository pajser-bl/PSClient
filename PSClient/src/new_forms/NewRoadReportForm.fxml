<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane styleClass="root7" stylesheets="@/css/background.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="new_forms.NewRoadReportController">
	<children>
		<AnchorPane fx:id="interventionAnchor" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label text="Terenski izvjestaj" textAlignment="CENTER" textFill="2a90a8" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
					<padding>
						<Insets left="10.0" />
					</padding>
				</Label>
				<VBox fx:id="interventionLabels" alignment="CENTER_LEFT" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">
					<children>
						<Label text="ID intervencije:" textFill="#21947b" />
						<Label text="Datum:" textFill="#21947b" />
						<Label text="Vrijeme:" textFill="#21947b" />
					</children>
					<padding>
						<Insets left="20.0" />
					</padding>
				</VBox>
				<VBox fx:id="interventionData" alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
					<padding>
						<Insets left="10.0" right="20.0" />
					</padding>
					<children>
						<Label fx:id="interventionId" text="label" textFill="#21947b" />
						<Label fx:id="date" text="label" textFill="#21947b" />
						<Label fx:id="time" text="label" textFill="#21947b" />
					</children>
				</VBox>
			</children>
		</AnchorPane>
		<AnchorPane fx:id="vehicleAnchor" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
			<children>
				<Label text="Podaci o vozilu:" textFill="2a90a8" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
					<padding>
						<Insets left="10.0" />
					</padding>
				</Label>
				<VBox fx:id="vehicleLabels" alignment="CENTER_LEFT" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">
					<children>
						<Label text="Registarski broj:" textFill="#21947b" />
						<Label text="Proizvodjac:" textFill="#21947b" />
						<Label text="Godina proizvodnje:" textFill="#21947b" />
						<Label text="Vrsta usluge:" textFill="#21947b" />
					</children>
					<padding>
						<Insets left="20.0" />
					</padding>
				</VBox>
				<VBox fx:id="vehicleData" alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
					<padding>
						<Insets left="10.0" right="20.0" />
					</padding>
					<children>
						<Label fx:id="licencePlate" text="label" textFill="#21947b" />
						<Label fx:id="manufacturer" text="label" textFill="#21947b" />
						<Label fx:id="yearMade" text="label" textFill="#21947b" />
						<ComboBox fx:id="service" prefWidth="150.0" />
					</children>
				</VBox>
			</children>
		</AnchorPane>
		<AnchorPane fx:id="reportAnchor" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
			<children>
				<Label text="Opis problema:" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
				<TextArea fx:id="report" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
				<Button fx:id="submitButton" mnemonicParsing="false" styleClass="commonbutton" stylesheets="@../css/common_button.css" text="Posalji" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
