<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane
	fx:controller="controller.operater.CloseInterventionController"
	styleClass="root7" stylesheets="@/css/background.css"
	xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/10.0.1">
	<children>
		<VBox spacing="20.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
			<children>
				<TextArea fx:id="reportText" />
				<Button fx:id="reportButton" defaultButton="true"
					mnemonicParsing="false" onAction="#sendReport"
					styleClass="commonbutton"
					stylesheets="@../../css/common_button.css" text="Posalji izvjestaj" />
			</children>
		</VBox>
	</children>
</AnchorPane>
