<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane styleClass="root7"
	stylesheets="@/css/background.css"
	xmlns="http://javafx.com/javafx/10.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.administrator.AddNewUserController">
	<children>
		<AnchorPane fx:id="topPane" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label text="Podaci o novom korisniku:" textFill="2a90a8"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0">
					<padding>
						<Insets left="10.0" />
					</padding>
				</Label>
				<AnchorPane fx:id="userDataLabels"
					AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
					AnchorPane.topAnchor="0.0">
					<children>
						<VBox alignment="CENTER_LEFT" spacing="40.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
							<children>
								<Label text="Ime korisnika:" textFill="#21947b" />
								<Label text="Prezime korisnika:" textFill="#21947b" />
								<Label text="Datum rodjenja:" textFill="#21947b" />
								<Label text="Strucna sprema" textFill="#21947b" />
								<Label text="Vozacka dozvola" textFill="#21947b" />
							</children>
							<padding>
								<Insets left="20.0" />
							</padding>
						</VBox>
					</children>
				</AnchorPane>
				<AnchorPane fx:id="userDataInput"
					AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0">
					<children>
						<VBox alignment="CENTER_LEFT" prefHeight="200.0"
							prefWidth="100.0" spacing="30.0" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0">
							<padding>
								<Insets left="10.0" right="20.0" />
							</padding>
							<children>
								<TextField fx:id="name" styleClass="text_field"
									stylesheets="@../../css/text_field.css" />
								<TextField fx:id="lastName" styleClass="text_field"
									stylesheets="@../../css/text_field.css" />
								<DatePicker fx:id="date" editable="false"
									styleClass="combo-box" stylesheets="@../../css/combo_box.css" />
								<ComboBox fx:id="qualification" prefWidth="150.0" styleClass="combo-box"
									value="VSS" stylesheets="@../../css/combo_box.css">
									<items>
										<FXCollections fx:factory="observableArrayList">
											<String fx:value="VSS" />
											<String fx:value="SSS" />
										</FXCollections>
									</items>
								</ComboBox>
								<TextField fx:id="licence" styleClass="text_field"
									stylesheets="@../../css/text_field.css" />
							</children>
						</VBox>
					</children>
				</AnchorPane>
			</children>
		</AnchorPane>
		<AnchorPane fx:id="bottomPane"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0">
			<children>
				<Label text="Podaci o novom korisnickom nalogu"
					textFill="2a90a8" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0">
					<padding>
						<Insets left="10.0" />
					</padding>
				</Label>
				<AnchorPane fx:id="accountSettingsPane"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
					<children>
						<AnchorPane fx:id="accountSettingsLabels"
							prefHeight="200.0" prefWidth="200.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.topAnchor="0.0">
							<children>
								<VBox prefHeight="200.0" prefWidth="100.0" spacing="40.0"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
									<children>
										<Label text="Nivo pristupa" textFill="#21947b" />
										<Label text="Korisnicko ime" textFill="#21947b" />
										<Label text="Lozinka" textFill="#21947b" />
									</children>
									<padding>
										<Insets left="20.0" top="20.0" />
									</padding>
								</VBox>
							</children>
						</AnchorPane>
						<AnchorPane fx:id="accountSettingsInput"
							prefHeight="200.0" prefWidth="200.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0">
							<children>
								<VBox prefHeight="200.0" prefWidth="100.0" spacing="35.0"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
									<children>
										<ComboBox fx:id="userType" prefWidth="150.0" styleClass="combo-box"
											value="Administrator" stylesheets="@../../css/combo_box.css">
											<items>
												<FXCollections
													fx:factory="observableArrayList">
													<String fx:value="Administrator" />
													<String fx:value="Supervizor" />
													<String fx:value="Operater" />
													<String fx:value="Terenski Radnik" />
												</FXCollections>
											</items>
										</ComboBox>
										<TextField fx:id="userName" styleClass="text_field"
											stylesheets="@../../css/text_field.css" />
										<TextField fx:id="password" styleClass="text_field"
											stylesheets="@../../css/text_field.css" />
									</children>
									<padding>
										<Insets top="15.0" />
									</padding>
								</VBox>
							</children>
							<padding>
								<Insets left="10.0" right="20.0" />
							</padding>
						</AnchorPane>
					</children>
				</AnchorPane>
				<Button mnemonicParsing="false" onAction="#createNewUser"
					styleClass="commonbutton" defaultButton="true"
					stylesheets="@../../css/common_button.css" text="Novi korisnik"
					AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="50.0" />
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
