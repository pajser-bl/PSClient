<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="root7" stylesheets="@/css/background.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.administrator.AddNewUserController">
	<children>
		<AnchorPane fx:id="userDataAnchor" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label text="Podaci o novom korisniku:" textFill="2a90a8" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
					<font>
						<Font size="20.0" />
					</font>
					<padding>
						<Insets left="10.0" />
					</padding>
				</Label>
				<VBox fx:id="userDataLabelsBox" alignment="CENTER_LEFT" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
					<children>
						<Label text="Ime korisnika:" textFill="#21947b" />
						<Label text="Prezime korisnika:" textFill="#21947b" />
						<Label text="Datum rodjenja:" textFill="#21947b" />
						<Label text="Strucna sprema" textFill="#21947b" />
						<Label text="Vozacka dozvola" textFill="#21947b" />
					</children>
					<padding>
						<Insets left="20.0" />
					</padding>
				</VBox>
				<VBox fx:id="userDataInputBox" alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
					<padding>
						<Insets left="10.0" right="20.0" />
					</padding>
					<children>
						<TextField fx:id="name" styleClass="text_field" stylesheets="@../../css/text_field.css" />
						<TextField fx:id="lastName" styleClass="text_field" stylesheets="@../../css/text_field.css" />
						<DatePicker fx:id="date" editable="false" stylesheets="@../../css/date_picker.css">
                     <styleClass>
                        <String fx:value="date-picker" />
                        <String fx:value="text-field" />
                     </styleClass></DatePicker>
						<ComboBox fx:id="qualification" prefWidth="150.0" styleClass="combo-box" stylesheets="@../../css/combo_box.css" value="VSS">
							<items>
								<FXCollections fx:factory="observableArrayList">
									<String fx:value="VSS" />
									<String fx:value="SSS" />
								</FXCollections>
							</items>
						</ComboBox>
						<TextField fx:id="licence" styleClass="text_field" stylesheets="@../../css/text_field.css" />
					</children>
				</VBox>
			</children>
		</AnchorPane>
		<AnchorPane fx:id="accountSettingsAnchor" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
			<children>
				<Label text="Podaci o novom korisnickom nalogu:" textFill="2a90a8" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0">
					<font>
						<Font size="20.0" />
					</font>
					<padding>
						<Insets left="10.0" />
					</padding>
				</Label>
				<VBox fx:id="accountSettingsLabelsBox" alignment="CENTER_LEFT" spacing="30.0" AnchorPane.leftAnchor="0.0">
					<children>
						<Label text="Nivo pristupa" textFill="#21947b" />
						<Label text="Korisnicko ime" textFill="#21947b" />
						<Label text="Lozinka" textFill="#21947b" />
					</children>
					<padding>
						<Insets left="20.0" />
					</padding>
				</VBox>
				<VBox fx:id="accountSettingsInputBox" alignment="CENTER_LEFT" spacing="20.0" AnchorPane.rightAnchor="0.0">
					<children>
						<ComboBox fx:id="userType" prefWidth="150.0" styleClass="combo-box" stylesheets="@../../css/combo_box.css" value="Administrator">
							<items>
								<FXCollections fx:factory="observableArrayList">
									<String fx:value="Administrator" />
									<String fx:value="Supervizor" />
									<String fx:value="Operater" />
									<String fx:value="Terenski Radnik" />
								</FXCollections>
							</items>
						</ComboBox>
						<TextField fx:id="username" styleClass="text_field" stylesheets="@../../css/text_field.css" />
						<TextField fx:id="password" styleClass="text_field" stylesheets="@../../css/text_field.css" />
					</children>
					<padding>
						<Insets left="10.0" right="20.0" />
					</padding>
				</VBox>
				<Button fx:id="createNewUser" defaultButton="true" mnemonicParsing="false" onAction="#createNewUser" styleClass="commonbutton" stylesheets="@../../css/common_button.css" text="Novi korisnik" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" />
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
