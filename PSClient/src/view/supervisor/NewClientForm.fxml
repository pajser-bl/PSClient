<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane styleClass="root7"
	stylesheets="@/css/background.css"
	xmlns="http://javafx.com/javafx/10.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.supervisor.NewClientController">
	<children>
		<VBox fx:id="labelBox" alignment="CENTER_LEFT" spacing="31.0"
			AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
			<children>
				<Label text="Ime:" textFill="#21947b" />
				<Label text="Prezime:" textFill="#21947b" />
				<Label text="Broj telefona:" textFill="#21947b" />
			</children>
		</VBox>
		<VBox fx:id="inputBox" alignment="CENTER_LEFT" spacing="20.0"
			AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
			<children>
				<TextField fx:id="name" styleClass="text_field"
					stylesheets="@../../css/text_field.css" />
				<TextField fx:id="lastName" styleClass="text_field"
					stylesheets="@../../css/text_field.css" />
				<TextField fx:id="phoneNumber" styleClass="text_field"
					stylesheets="@../../css/text_field.css" />
			</children>
		</VBox>
		<Button fx:id="createButton" mnemonicParsing="false"
			onAction="#createNewClient" styleClass="commonbutton"
			stylesheets="@../../css/common_button.css" text="Napravi"
			AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" />
	</children>
</AnchorPane>
